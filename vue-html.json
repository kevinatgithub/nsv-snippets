			"<!--Requires Scripting-->",
			"<DatePicker :year=\"${currentYear}\" :month=\"${currentMonth}\" :day=\"${currentDay}\" minDate=\"${MinDate}\" maxDate=\"MaxDate\" />"
		]
	},
	"NativeScript DataForm" : {
		"prefix" : "nsdataform",
